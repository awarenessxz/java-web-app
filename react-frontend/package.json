  "name": "react-frontend",
  "version": "1.0.0",
  "description": "Simple React Web Application",
  "main": "index.js",
  "scripts": {
    "analyze": "webpack-bundle-analyzer --port 4200 stats.json",
    "build": "webpack --env env=production",
    "start": "webpack serve --env env=development",
    "stats": "webpack --env production --mode production --profile --json | sed '1,2d' > stats.json && yarn run analyze",
    "lint:js": "eslint */**/*.{ts,tsx} --quiet",
    "lint:js-fix": "yarn lint:js -- --fix",
    "format": "prettier --write --single-quote --trailing-comma=all --print-width 120 --tab-width 4 \"src/**/*.{ts,tsx}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/awarenessxz/java-web-app.git"
  },
