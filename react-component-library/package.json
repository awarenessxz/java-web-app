{
    "name": "react-component-library",
    "version": "1.0.0",
    "description": "React Library which contains a collection of custom reusable React Components, documented with storybook and tested using Jest &amp; React-Testing-Library",
    "main": "dist/cjs/index.js",
    "module": "dist/esm/index.js",
    "source": {
        "index": "./src/index.ts",
        "TestComponent": "./src/TestComponent/TestComponent"
    },
    "target": {
        "cjs": "dist/cjs",
        "esm": "dist/esm"
    },
    "files": [
        "dist/cjs/*",
        "dist/esm/*"
    ],
    "scripts": {
        "test": "jest",
        "test:watch": "jest --watch",
        "build": "yarn run build:clean && yarn run build:generate && rollup -c",
        "build:clean": "rm -rf dist",
        "build:generate": "node ./util/auto-generate-entry-points ./src/index.ts",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook",
        "lint": "eslint .",
        "lint:format": "prettier --write ."
    },
    "engines": {
        "node": ">=10"
    },
    "keywords": [
        "react",
        "components",
        "custom",
        "library",
        "typescript",
        "storybook"
    ],
    "author": "awarenessxz",
    "license": "Apache-2.0",
    "peerDependencies": {
        "react": ">=16.8.0",
        "react-dom": ">=16.8.0"
    },
    "devDependencies": {
        "@babel/core": "^7.12.1",
        "@rollup/plugin-commonjs": "^15.1.0",
        "@rollup/plugin-node-resolve": "^8.0.0",
        "@storybook/addon-console": "^1.2.2",
        "@storybook/addon-essentials": "^6.0.26",
        "@storybook/addon-links": "^6.0.26",
        "@storybook/addon-storysource": "^6.0.26",
        "@storybook/react": "^6.0.26",
        "@types/react": "^16.9.52",
        "autoprefixer": "^10.0.1",
        "babel-loader": "^8.1.0",
        "node-sass": "^4.14.1",
        "react": "^16.14.0",
        "react-dom": "^16.14.0",
        "react-is": "^16.13.1",
        "rollup": "^2.31.0",
        "rollup-plugin-peer-deps-external": "^2.2.3",
        "rollup-plugin-postcss": "^3.1.8",
        "rollup-plugin-typescript2": "^0.27.3",
        "sass-loader": "^10.0.3",
        "typescript": "^4.0.3"
    },
    "dependencies": {}
}